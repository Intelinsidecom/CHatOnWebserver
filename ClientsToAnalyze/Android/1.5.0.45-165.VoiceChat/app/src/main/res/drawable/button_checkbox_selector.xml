<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="false"
        android:state_checked="false"
        android:drawable="@drawable/uncheck_dim"/>
    <item
        android:state_focused="true"
        android:state_checked="false"
        android:drawable="@drawable/uncheck_focus"/>
    <item
        android:state_checked="false"
        android:state_pressed="true"
        android:drawable="@drawable/uncheck_press"/>
    <item
        android:state_enabled="true"
        android:state_checked="false"
        android:drawable="@drawable/uncheck_normal"/>
    <item
        android:state_enabled="false"
        android:state_checked="true"
        android:drawable="@drawable/check_dim"/>
    <item
        android:state_focused="true"
        android:state_checked="true"
        android:drawable="@drawable/check_focus"/>
    <item
        android:state_checked="true"
        android:state_pressed="true"
        android:drawable="@drawable/check_press"/>
    <item
        android:state_enabled="true"
        android:state_checked="true"
        android:drawable="@drawable/check_normal"/>
</selector>
