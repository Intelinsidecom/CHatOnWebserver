<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="false"
        android:state_selected="true"
        android:drawable="@drawable/input_filed_select_disabled"/>
    <item
        android:state_enabled="true"
        android:state_selected="true"
        android:drawable="@drawable/input_filed_selected"/>
    <item
        android:state_pressed="true"
        android:drawable="@drawable/input_filed_pressed"/>
    <item
        android:state_focused="true"
        android:drawable="@drawable/input_filed_selected"/>
    <item
        android:state_enabled="false"
        android:drawable="@drawable/input_filed_normal_disabled"/>
    <item
        android:state_enabled="true"
        android:drawable="@drawable/input_filed_normal"/>
</selector>
