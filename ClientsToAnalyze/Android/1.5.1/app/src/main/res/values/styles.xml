<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="seekbar_style" parent="">
        <item name="android:maxHeight">10dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:minHeight">10dp</item>
    </style>
    <style name="Bg" parent="">
    </style>
    <style name="Bg.Main" parent="@style/Bg">
        <item name="android:windowBackground">@color/white</item>
    </style>
    <style name="Bg.Basic" parent="@style/Bg">
        <item name="android:windowBackground">@color/chat_backgroun</item>
    </style>
    <style name="Bg.Wallpaper" parent="@android:style/Theme.Wallpaper">
        <item name="android:colorForeground">#fff</item>
    </style>
    <style name="Bg.Chat" parent="@style/Bg">
        <item name="android:windowBackground">@color/gray</item>
    </style>
    <style name="Bg.List" parent="@style/Bg">
        <item name="android:windowBackground">@color/hotpink</item>
    </style>
    <style name="PartsDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="MainCustomDialogDefaultButton" parent="@android:style/Widget.Button">
        <item name="android:textAppearance">?android:attr/textAppearanceSmallInverse</item>
        <item name="android:textColor">@color/main_custome_default_button_text</item>
        <item name="android:gravity">0x11</item>
        <item name="android:background">@drawable/ams_btn_default</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="SettingsDialogDefaultButton" parent="@android:style/Widget.Button">
        <item name="android:textAppearance">?android:attr/textAppearanceSmallInverse</item>
        <item name="android:textColor">@color/ams_default_button_text</item>
        <item name="android:gravity">0x11</item>
        <item name="android:background">@drawable/co_btn_default_normal</item>
    </style>
    <style name="Animations" parent="">
    </style>
    <style name="Animations.PopDownMenu" parent="@style/Animations">
    </style>
    <style name="Animations.PopDownMenu.Left" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_topleft_to_bottomright</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_bottomright_to_topleft</item>
    </style>
    <style name="Animations.PopDownMenu.Right" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_topright_to_bottomleft</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_bottomleft_to_topright</item>
    </style>
    <style name="Animations.PopDownMenu.Center" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_top</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_bottom</item>
    </style>
    <style name="Animations.PopUpMenu" parent="@style/Animations">
    </style>
    <style name="Animations.PopUpMenu.Left" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_bottomleft_to_topright</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_topright_to_bottomleft</item>
    </style>
    <style name="Animations.PopUpMenu.Right" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_bottomright_to_topleft</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_topleft_to_bottomright</item>
    </style>
    <style name="Animations.PopUpMenu.Center" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_top</item>
    </style>
    <style name="Theme.Translucent" parent="@android:style/Theme.Translucent">
        <item name="android:colorForeground">#fff</item>
        <item name="android:buttonStyle">@style/MainCustomDialogDefaultButton</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:alertDialogStyle">@style/MainAlertDialogTheme</item>
        <item name="android:editTextStyle">@style/AMSCustomEditTextTheme</item>
        <item name="android:tabWidgetStyle">@style/AMSCustomTabWidgetTheme</item>
    </style>
    <style name="CustomWindowTitleText" parent="@android:style/TextAppearance.WindowTitle">
        <item name="android:textStyle">0x1</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="CustomWindowTitleBackground" parent="">
        <item name="android:background">@drawable/co_title_bar</item>
    </style>
    <style name="CustomWindowTitle" parent="@style/WindowTitle">
        <item name="android:textAppearance">@style/CustomWindowTitleText</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:shadowColor">#1155cc</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">0</item>
    </style>
    <style name="CustomTheme" parent="@android:style/Theme">
        <item name="android:windowTitleSize">25dp</item>
        <item name="android:windowTitleStyle">@style/CustomWindowTitle</item>
        <item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
    </style>
    <style name="MainCustomTheme" parent="@android:style/Theme">
        <item name="android:buttonStyle">@style/MainCustomDialogDefaultButton</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:alertDialogStyle">@style/MainAlertDialogTheme</item>
        <item name="android:editTextStyle">@style/AMSCustomEditTextTheme</item>
        <item name="android:tabWidgetStyle">@style/AMSCustomTabWidgetTheme</item>
    </style>
    <style name="MainCustomTheme.NoTitle" parent="@style/MainCustomTheme">
    </style>
    <style name="SettingsCustomTheme" parent="@android:style/Theme">
        <item name="android:buttonStyle">@style/SettingsDialogDefaultButton</item>
        <item name="android:windowTitleSize">25dp</item>
        <item name="android:windowTitleStyle">@style/CustomWindowTitle</item>
        <item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
        <item name="android:alertDialogStyle">@style/MainAlertDialogTheme</item>
    </style>
    <style name="NoTitleBackground" parent="@style/WindowTitleBackground">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="MainAlertDialogTheme" parent="@style/AlertDialog">
        <item name="android:buttonStyle">@style/SettingsPopUpDefaultButton</item>
        <item name="android:windowBackground">@drawable/buddy_item_xml</item>
        <item name="android:fullDark">@drawable/popup_full_dark</item>
        <item name="android:topDark">@drawable/popup_top_dark</item>
        <item name="android:centerDark">@drawable/popup_center_dark</item>
        <item name="android:bottomDark">@drawable/popup_bottom_dark</item>
        <item name="android:fullBright">@drawable/popup_full_bright</item>
        <item name="android:topBright">@drawable/popup_top_bright</item>
        <item name="android:centerBright">@drawable/popup_center_bright</item>
        <item name="android:bottomBright">@drawable/popup_bottom_bright</item>
        <item name="android:bottomMedium">@drawable/popup_bottom_medium</item>
        <item name="android:centerMedium">@drawable/popup_center_medium</item>
    </style>
    <style name="SettingsPopUpDefaultButton" parent="@android:style/Widget.Button">
        <item name="android:textAppearance">?android:attr/textAppearanceSmallInverse</item>
        <item name="android:textColor">@color/ams_default_button_text</item>
        <item name="android:gravity">0x11</item>
        <item name="android:background">@drawable/ams_popup_btn_default</item>
    </style>
    <style name="AMSCustomTheme" parent="@android:style/Theme">
        <item name="android:buttonStyle">@style/AMSDefaultButton</item>
        <item name="android:windowTitleSize">25dp</item>
        <item name="android:windowTitleStyle">@style/CustomWindowTitle</item>
        <item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
        <item name="android:alertDialogStyle">@style/AMSCustomAlertDialogTheme</item>
        <item name="android:editTextStyle">@style/AMSCustomEditTextTheme</item>
        <item name="android:radioButtonStyle">@style/AMSRadioButton</item>
        <item name="android:tabWidgetStyle">@style/AMSCustomTabWidgetTheme</item>
    </style>
    <style name="AMSCustomDialogTheme" parent="@android:style/Theme.Dialog">
        <item name="android:buttonStyle">@style/AMSPopUpDefaultButton</item>
        <item name="android:windowBackground">@drawable/popup_full_dark</item>
    </style>
    <style name="AMSCustomAlertDialogTheme" parent="@style/AlertDialog">
        <item name="android:buttonStyle">@style/AMSPopUpDefaultButton</item>
        <item name="android:fullDark">@drawable/popup_full_dark</item>
        <item name="android:topDark">@drawable/popup_top_dark</item>
        <item name="android:centerDark">@drawable/popup_center_dark</item>
        <item name="android:bottomDark">@drawable/popup_bottom_dark</item>
        <item name="android:fullBright">@drawable/popup_full_bright</item>
        <item name="android:topBright">@drawable/popup_top_bright</item>
        <item name="android:centerBright">@drawable/popup_center_bright</item>
        <item name="android:bottomBright">@drawable/popup_bottom_bright</item>
        <item name="android:bottomMedium">@drawable/popup_bottom_medium</item>
        <item name="android:centerMedium">@drawable/popup_center_medium</item>
    </style>
    <style name="AMSCustomEditTextTheme" parent="@android:style/Widget.EditText">
        <item name="android:background">@drawable/ams_edit_text</item>
    </style>
    <style name="AMSCustomTabWidgetTheme" parent="@android:style/Widget.TabWidget">
        <item name="android:textAppearance">@style/AMSCustomTabText</item>
    </style>
    <style name="AMSCustomTabText" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:textColor">@color/ams_tab_indicator_text</item>
    </style>
    <style name="AMSDefaultButton" parent="@android:style/Widget.Button">
        <item name="android:textAppearance">?android:attr/textAppearanceSmallInverse</item>
        <item name="android:textColor">@color/ams_default_button_text</item>
        <item name="android:gravity">0x11</item>
        <item name="android:background">@drawable/ams_btn_default</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="AMSPopUpDefaultButton" parent="@android:style/Widget.Button">
        <item name="android:textAppearance">?android:attr/textAppearanceSmallInverse</item>
        <item name="android:textColor">@color/ams_default_button_text</item>
        <item name="android:gravity">0x11</item>
        <item name="android:background">@drawable/ams_popup_btn_default</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="AMSRadioButton" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/ams_btn_radio</item>
    </style>
    <style name="WindowTitleBackground" parent="">
        <item name="android:background">@android:drawable/title_bar</item>
    </style>
    <style name="WindowTitle" parent="">
        <item name="android:textAppearance">@android:style/TextAppearance.WindowTitle</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">#bb000000</item>
        <item name="android:shadowRadius">2.75</item>
    </style>
    <style name="DialogWindowTitle" parent="">
        <item name="android:textAppearance">@android:style/TextAppearance.DialogWindowTitle</item>
        <item name="android:maxLines">1</item>
        <item name="android:scrollHorizontally">true</item>
    </style>
    <style name="AlertDialog" parent="">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleStyle">@style/DialogWindowTitle</item>
    </style>
    <style name="TrunkCustomTheme" parent="@style/MainCustomTheme">
        <item name="android:windowNoTitle">false</item>
        <item name="android:windowTitleSize">25dp</item>
        <item name="android:windowTitleStyle">@style/CustomWindowTitle</item>
        <item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
    </style>
</resources>
