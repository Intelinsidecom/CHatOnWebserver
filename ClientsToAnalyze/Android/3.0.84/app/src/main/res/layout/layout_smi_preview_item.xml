<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/trunk_image_view_loading_bg"
    android:layout_width="match_parent"
    android:layout_height="@dimen/trunk_list_item_height">
    <ImageView
        android:id="@+id/thumbnailPreivew"
        android:layout_width="@dimen/trunk_list_item_height"
        android:layout_height="@dimen/trunk_list_item_height"
        android:scaleType="centerCrop"/>
    <ImageView
        android:id="@+id/deleteCheck"
        android:background="@drawable/anicon_package_item_downloading_event_xml"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"/>
</RelativeLayout>
