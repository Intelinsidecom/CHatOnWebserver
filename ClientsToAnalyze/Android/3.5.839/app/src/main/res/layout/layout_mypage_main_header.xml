<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:local="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/llayout_mypage_coverstory_press_area"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:cacheColorHint="#00000000">
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/mypage_main_header_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="9dp">
        <FrameLayout
            android:id="@+id/mypage_main_profile_image_represent_layout"
            android:background="@drawable/thum_stroke"
            android:paddingLeft="3dp"
            android:paddingTop="3dp"
            android:paddingRight="0dp"
            android:paddingBottom="3.5dp"
            android:layout_width="93dp"
            android:layout_height="96.5dp">
            <com.sec.chaton.widget.SelectableImageView
                android:id="@+id/mypage_main_profile_image_represent"
                android:focusable="true"
                android:nextFocusRight="@+id/mypage_main_profile_edit_button"
                android:clickable="true"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:contentDescription="@string/mypage_profile_image"
                local:imageViewSelector="@drawable/imageview_selector"/>
            <ImageView
                android:layout_gravity="center"
                android:id="@+id/profile_image_loading"
                android:visibility="gone"
                android:layout_width="40dp"
                android:layout_height="40dp"/>
            <ImageView
                android:layout_gravity="bottom|left"
                android:id="@+id/mypage_main_profile_image_camera"
                android:padding="0dp"
                android:visibility="gone"
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:layout_marginLeft="4dp"
                android:layout_marginBottom="4dp"
                android:src="@drawable/mypage_btn_icon_edit"/>
        </FrameLayout>
        <LinearLayout
            android:gravity="bottom"
            android:orientation="vertical"
            android:background="@drawable/co_list_bg"
            android:padding="0dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="96.5dp">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/mypage_name_status_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="4dp"
                android:layout_marginRight="10dp"
                android:minHeight="37dp">
                <TextView
                    android:textAppearance="@style/TextAppearance.List01"
                    android:ellipsize="end"
                    android:id="@+id/mypage_main_profile_name"
                    android:focusable="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:maxLines="3"
                    android:maxLength="30"/>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_gravity="top"
                        android:id="@+id/mypage_main_profile_status_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="7dp"
                        android:src="@drawable/buddies_videocall"/>
                    <TextView
                        android:textAppearance="@style/TextAppearance.List17"
                        android:ellipsize="end"
                        android:id="@+id/mypage_main_profile_status_message"
                        android:focusable="true"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:minHeight="20dp"
                        android:maxLines="3"
                        android:layout_weight="1"/>
                </LinearLayout>
            </LinearLayout>
            <include
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="3dp"
                android:layout_marginRight="7dp"
                layout="@layout/layout_common_divider5"/>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5.5dp">
                    <FrameLayout
                        android:descendantFocusability="afterDescendants"
                        android:layout_width="@dimen/mypage_small_profile_imamge"
                        android:layout_height="@dimen/mypage_small_profile_imamge"
                        android:layout_marginLeft="6dp">
                        <com.sec.chaton.widget.SelectableImageView
                            android:id="@+id/mypage_main_profile_image_sub1"
                            android:padding="1dp"
                            android:focusable="true"
                            android:clickable="true"
                            android:layout_width="@dimen/mypage_small_profile_imamge"
                            android:layout_height="@dimen/mypage_small_profile_imamge"
                            android:scaleType="fitXY"
                            android:contentDescription="@string/mypage_profile_image"
                            local:imageViewSelector="@drawable/imageview_selector"/>
                        <ImageView
                            android:layout_gravity="center"
                            android:id="@+id/profile_image_1_loading"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"/>
                    </FrameLayout>
                    <FrameLayout
                        android:descendantFocusability="afterDescendants"
                        android:layout_width="@dimen/mypage_small_profile_imamge"
                        android:layout_height="@dimen/mypage_small_profile_imamge"
                        android:layout_marginLeft="@dimen/mypage_small_profile_imamge_margin">
                        <com.sec.chaton.widget.SelectableImageView
                            android:id="@+id/mypage_main_profile_image_sub2"
                            android:padding="1dp"
                            android:focusable="true"
                            android:clickable="true"
                            android:layout_width="@dimen/mypage_small_profile_imamge"
                            android:layout_height="@dimen/mypage_small_profile_imamge"
                            android:scaleType="fitXY"
                            android:contentDescription="@string/mypage_profile_image"
                            local:imageViewSelector="@drawable/imageview_selector"/>
                        <ImageView
                            android:layout_gravity="center"
                            android:id="@+id/profile_image_2_loading"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"/>
                    </FrameLayout>
                    <FrameLayout
                        android:descendantFocusability="afterDescendants"
                        android:layout_width="@dimen/mypage_small_profile_imamge"
                        android:layout_height="@dimen/mypage_small_profile_imamge"
                        android:layout_marginLeft="@dimen/mypage_small_profile_imamge_margin">
                        <com.sec.chaton.widget.SelectableImageView
                            android:id="@+id/mypage_main_profile_image_sub3"
                            android:padding="1dp"
                            android:focusable="true"
                            android:clickable="true"
                            android:layout_width="@dimen/mypage_small_profile_imamge"
                            android:layout_height="@dimen/mypage_small_profile_imamge"
                            android:scaleType="fitXY"
                            android:contentDescription="@string/mypage_profile_image"
                            local:imageViewSelector="@drawable/imageview_selector"/>
                        <ImageView
                            android:layout_gravity="center"
                            android:id="@+id/profile_image_3_loading"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"/>
                    </FrameLayout>
                    <ImageView
                        android:id="@+id/mypage_profile_more"
                        android:background="?attr/itemBackground"
                        android:focusable="true"
                        android:visibility="gone"
                        android:clickable="true"
                        android:layout_width="23dp"
                        android:layout_height="32dp"
                        android:layout_marginLeft="@dimen/mypage_arrow_margin"
                        android:src="@drawable/chat_icon_arrow"
                        android:scaleType="centerInside"
                        android:contentDescription="@string/view_more"/>
                </LinearLayout>
                <FrameLayout
                    android:gravity="right"
                    android:id="@+id/mypage_info_layout"
                    android:descendantFocusability="afterDescendants"
                    android:layout_width="match_parent"
                    android:layout_height="43.5dp"
                    android:dividerPadding="5dp">
                    <ImageButton
                        android:layout_gravity="right"
                        android:id="@+id/btn_mypage_info"
                        android:focusable="true"
                        android:layout_width="@dimen/mypage_info_button_width"
                        android:layout_height="@dimen/mypage_info_button_height"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/registration_ic_edit"
                        android:contentDescription="@string/mypage_info"
                        style="@style/ImageButtonStyle7"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_gravity="right"
        android:orientation="vertical"
        android:id="@+id/mypage_info_text_layout"
        android:layout_width="232dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="5.5dp"
        android:layout_marginRight="19dp">
        <TextView
            android:textAppearance="@style/MyPageInfoTextStyle"
            android:gravity="right"
            android:id="@+id/mypage_info_phonenumber1"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:shadowColor="#000000"
            android:shadowRadius="5"/>
        <TextView
            android:textAppearance="@style/MyPageInfoTextStyle"
            android:gravity="right"
            android:id="@+id/mypage_info_phonenumber2"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:shadowColor="#000000"
            android:shadowRadius="5"/>
        <TextView
            android:textAppearance="@style/MyPageInfoTextStyle"
            android:gravity="right"
            android:id="@+id/mypage_info_phonenumber3"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:shadowColor="#000000"
            android:shadowRadius="5"/>
        <TextView
            android:textAppearance="@style/MyPageInfoTextStyle"
            android:gravity="right"
            android:id="@+id/mypage_info_phonenumber4"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:shadowColor="#000000"
            android:shadowRadius="5"/>
        <LinearLayout
            android:gravity="right"
            android:orientation="horizontal"
            android:id="@+id/mypage_samsungaccount_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textAppearance="@style/MyPageInfoTextStyle"
                android:id="@+id/mypage_info_samsungaccount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:shadowColor="#000000"
                android:shadowRadius="5"/>
        </LinearLayout>
        <LinearLayout
            android:gravity="right"
            android:orientation="horizontal"
            android:id="@+id/mypage_birthday_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="3dp"
                android:src="@drawable/mypage_info_birthday"/>
            <TextView
                android:textAppearance="@style/MyPageInfoTextStyle"
                android:id="@+id/mypage_info_birthday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:shadowColor="#000000"
                android:shadowRadius="5"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:paddingLeft="9dp"
        android:paddingRight="7.5dp"
        android:paddingBottom="9dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="97.5dp">
        <RelativeLayout
            android:id="@+id/mypage_poston_write_frame"
            android:background="@drawable/frame_selector"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textAppearance="@style/TextAppearance.List19"
                android:gravity="center_vertical"
                android:id="@+id/mypage_poston_write_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="1dp"
                android:text="@string/poston_title"
                android:shadowColor="#000000"
                android:shadowRadius="2.5"/>
            <ImageView
                android:id="@+id/mypage_main_poston_write"
                android:background="@drawable/mypage_edit_btn_selector"
                android:layout_width="17dp"
                android:layout_height="17dp"
                android:layout_marginTop="4.5dp"
                android:layout_toLeftOf="@+id/mypage_write_text"
                android:contentDescription="@string/mypage_poston_write"/>
            <TextView
                android:textAppearance="@style/TextAppearance.List69"
                android:gravity="center_vertical"
                android:id="@+id/mypage_write_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="4.5dp"
                android:layout_marginRight="1dp"
                android:text="@string/mypage_poston_write"
                android:shadowColor="#000000"
                android:shadowRadius="2.5"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>
        <ImageView
            android:background="@drawable/mypage_line_poston_write"
            android:layout_width="match_parent"
            android:layout_height="6dp"
            android:layout_marginTop="1dp"/>
    </LinearLayout>
</LinearLayout>
