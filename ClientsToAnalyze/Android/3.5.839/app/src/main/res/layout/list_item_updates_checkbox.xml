<?xml version="1.0" encoding="utf-8"?>
<com.sec.chaton.widget.CheckableRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:local="http://schemas.android.com/apk/res-auto"
    android:paddingLeft="7dp"
    android:paddingTop="@dimen/li6_padding_top"
    android:paddingRight="17dp"
    android:paddingBottom="@dimen/li6_padding_bottom"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="@dimen/li6_min_height"
    local:checkable_widget="@android:id/checkbox">
    <ImageView
        android:id="@+id/image1"
        android:layout_width="40dp"
        android:layout_height="40.5dp"
        android:layout_marginRight="15dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"/>
    <CheckBox
        android:id="@android:id/checkbox"
        android:focusable="false"
        android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/li6_checkbox1_layout_margin_left"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        style="@style/CheckBoxStyle.WithoutText"/>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/floor4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@android:id/checkbox"
        android:layout_centerVertical="true">
        <com.sec.chaton.widget.AdaptableTextView
            android:textAppearance="@style/TextAppearance.List53"
            android:layout_gravity="right"
            android:id="@+id/text3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <com.sec.chaton.widget.AdaptableTextView
            android:textAppearance="@style/TextAppearance.List53"
            android:layout_gravity="right"
            android:id="@+id/text4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/content_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="12dp"
        android:layout_toLeftOf="@+id/floor4"
        android:layout_toRightOf="@+id/image1"
        android:layout_centerVertical="true">
        <LinearLayout
            android:id="@+id/floor1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.sec.chaton.widget.AdaptableTextView
                android:textAppearance="@style/TextAppearance.List67"
                android:ellipsize="end"
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <FrameLayout
                android:id="@+id/space3"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/floor2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">
            <com.sec.chaton.widget.AdaptableTextView
                android:textAppearance="@style/TextAppearance.List09"
                android:ellipsize="end"
                android:id="@+id/text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="4"/>
            <FrameLayout
                android:id="@+id/space4"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/floor3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="2dp">
            <ImageView
                android:id="@+id/icon1"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/icon2"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"/>
            <FrameLayout
                android:id="@+id/space5"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>
    </LinearLayout>
</com.sec.chaton.widget.CheckableRelativeLayout>
