<?xml version="1.0" encoding="utf-8"?>
<com.sec.chaton.smsplugin.p112ui.SlideshowAttachmentView xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:id="@+id/slideshow_attachment_view"
    android:background="#fff"
    android:paddingTop="9dp"
    android:paddingRight="5dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/slideshow_image"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:layout_marginLeft="105dp"/>
    <TextView
        android:textColor="@drawable/text_color_black"
        android:id="@+id/slideshow_text"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <ImageButton
        android:id="@+id/remove_slideshow_button"
        android:background="@drawable/anicon_package_item_downloading_event_xml"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="6dp"/>
</com.sec.chaton.smsplugin.p112ui.SlideshowAttachmentView>
